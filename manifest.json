{
    "manifest_version":3,
    "name": "Website-BLocker-Extension",
    "version": "1.0",
    "description": "Extension to block websites",
    "permissions": [
        "declarativeNetRequest",
        "tabs",
        "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "background":{
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icons8-blocker-64.png",
        "default_popup": "popup.html"
    },
    "incognito": "split"

}