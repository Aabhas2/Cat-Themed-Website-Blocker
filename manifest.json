{
    "manifest_version":3,
    "name": "Website-BLocker-Extension",
    "version": "1.0",
    "description": "Extension to block websites",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage"
    ],
    "background":{
        "service_worker": "background.js"
    },
    "browser-action": {
        "default-icon": "icon.png",
        "default-popup": "index.html"
    },
    "host-permissions": ["<all_urls>"],
    "incognito": "split"

}